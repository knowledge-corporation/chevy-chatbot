      "**/*"
    ],
    "android": {
      "package": "com.knowledge.chevy",
      "versionCode": 1,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8494738329887200~4318815237"
      }
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8494738329887200~4318815237"
      }
    },
    "description": "The Chevy Chatbot aims to provide users with the possibility of conducting dialogues in English where the conversations and answers are registered and evaluated by the users themselves.",
    "githubUrl": "https://github.com/barbosamaatheus/chevy-chatbot"
